   if test x${CDSTL} = x ; then
      CDSTL=`${CDRECORD} -scanbus 2>/dev/null | grep CD+RW | ${AWK} '{print $1}'`
   fi
   if test x${CDSTL} = x ; then
      CDSTL="3,0,0"
   fi
