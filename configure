echo "${ECHO_T}no" >&6
              { { echo "$as_me:$LINENO: error: Unable to find libpq-fe.h in standard locations" >&5
echo "$as_me: error: Unable to find libpq-fe.h in standard locations" >&2;}
   { (exit 1); exit 1; }; }
          fi
