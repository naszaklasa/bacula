# Some AWK programs fail, so test it and warn the user
if echo xfoo | $AWK 'BEGIN { prog=ARGV1; ARGC=1 }
      { if ((prog == $2) || (("(" prog ")") == $2) ||
	   (("" prog "") == $2) ||
	   ((prog ":") == $2)) { print $1 ; exit 0 } }' xfoo>/dev/null; then :;
else
